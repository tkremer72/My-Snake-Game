{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "curly": "error",
        "multiline-comment-style": "error",
        "no-alert": "error",
        "no-console": "error",
        "no-duplicate-imports": "error",
        "quotes": ["error", "single", "double"],
        "semi": "error",
        "sort-imports": "error",
        "strict": "error"
    }
}
